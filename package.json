{
  "name": "operation-turbo-2023",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s download:manifest next:dev",
    "next:dev": "next dev",
    "build": "run-s download:manifest next:build",
    "next:build": "next build",
    "download:manifest": "uniform context manifest download --output ./lib/uniform/contextManifest.json",
    "uniform:pull:signals": "uniform context signal pull ./content/signals --mode createOrUpdate",
    "uniform:pull:components": "uniform canvas component pull ./content/components --mode createOrUpdate",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions --mode createOrUpdate",
    "uniform:pull": "run-s uniform:pull:signals uniform:pull:components uniform:pull:compositions",
    "uniform:push:signals": "uniform context signal push ./content/signals --mode createOrUpdate",
    "uniform:push:components": "uniform canvas component push ./content/components --mode createOrUpdate",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions --mode createOrUpdate",
    "uniform:publish": "uniform canvas composition push ./content/compositions --state published --mode createOrUpdate",
    "uniform:push": "run-s uniform:push:signals uniform:push:components uniform:push:compositions ",
    "start": "next start",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@cloudinary-util/url-loader": "^3.2.0",
    "@next/font": "^13.3.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@uniformdev/canvas": "^19.0.0",
    "@uniformdev/canvas-cloudinary": "^18.29.0",
    "@uniformdev/canvas-next": "^19.0.0",
    "@uniformdev/canvas-react": "^19.0.0",
    "@uniformdev/cli": "^19.0.0",
    "@uniformdev/context": "^19.0.0",
    "@uniformdev/context-next": "^19.0.0",
    "@uniformdev/context-react": "^19.0.0",
    "@uniformdev/project-map": "^19.0.0",
    "autoprefixer": "^10.4.14",
    "cloudinary-build-url": "^0.2.4",
    "contentful": "^10.1.3",
    "next": "^13.3.1",
    "next-cloudinary": "^4.4.0",
    "next-plausible": "^3.7.2",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.0.7",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@types/react": "^18.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  }
}
